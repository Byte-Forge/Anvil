STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
INPUT                  = @PROJECT_SOURCE_DIR@/src \
                         @PROJECT_SOURCE_DIR@/doc
FILE_PATTERNS          = *.hpp *.cpp *.dox
RECURSIVE              = YES
